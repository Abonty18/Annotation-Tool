{
  "version": 2,
  "builds": [
    {
      "src": "myproject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.bat",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/myproject/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myproject.settings",
    "PYTHONPATH": "myproject"
  },
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "myproject.settings",
      "PYTHONPATH": "myproject"
    }
  }
}
