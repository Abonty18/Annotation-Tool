{
     "version": 2,
     "builds": [
       {
         "src": "myproject/wsgi.py",
         "use": "@vercel/python",
         "config": {
           "maxLambdaSize": "50mb"
         }
       },
       {
         "src": "build_files.sh",
         "use": "@vercel/static-build",
         "config": {
           "distDir": "staticfiles_build"
         }
       }
     ],
     "routes": [
       {
         "src": "/static/(.*)",
         "headers": {
           "cache-control": "s-maxage=31536000,immutable"
         },
         "dest": "/staticfiles_build/$1"
       },
       {
         "src": "/(.*)",
         "dest": "/myproject/wsgi.py"
       }
     ],
     "env": {
       "DJANGO_SETTINGS_MODULE": "myproject.settings"
     },
     "build": {
       "env": {
         "DJANGO_SETTINGS_MODULE": "myproject.settings"
       }
     }
   }
   